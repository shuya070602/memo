<div class="container">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3">
      <h2>会員一覧</h2>
      <!--検索機能の表示-->
      <%= render 'users/search' %>

        <table class="table">
          <tr>
            <th>名前</th>
          </tr>

          <% @users.each do |user| %>
          <tr>
            <td><%= user.name%></td>
          </tr>
          <% end %>
        </table>
    </div>
  </div>
</div>